model Project {
  id             String   @id @default(uuid())
  name           String
  
  // Relations...
  
  organizationId String
  organization   Organization @relation(fields: [organizationId], references: [id], onDelete: Cascade)
  
  tasks          Task[]

  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt

  @@map("projects")
}